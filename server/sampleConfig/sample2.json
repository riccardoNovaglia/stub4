{
  "cruds": [
    {
      "meta": { "url": "/some-other" },
      "data": [{ "id": 1, "some": "content" }, { "id": 3, "some": "stuff" }]
    }
  ],
  "scenarios": [
    {
      "matching": { "url": "/with-body", "body": { "customerId": "*" } },
      "outcomes": [
        {
          "match": { "customerId": "1" },
          "response": { "body": { "name": "jimbo" }, "statusCode": 200 }
        }
      ],
      "default": {
        "response": { "body": {}, "statusCode": 404 }
      }
    }
  ]
}
