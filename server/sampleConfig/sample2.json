{
  "cruds": [
    {
      "requestMatcher": { "url": "/some-other" },
      "data": [{ "id": 1, "some": "content" }, { "id": 3, "some": "stuff" }]
    }
  ],
  "scenarios": [
    {
      "requestMatcher": { "url": "/with-body", "body": { "customerId": "*" } },
      "outcomes": [
        {
          "match": { "customerId": "1" },
          "response": { "body": { "name": "jimbo" }, "statusCode": 200 }
        }
      ],
      "default": {
        "response": { "body": {}, "statusCode": 404 }
      }
    }
  ],
  "proxy": [
    {
      "requestMatcher": { "url": "/to-be-proxied" },
      "method": "GET",
      "destination": { "url": "http://localhost:9090/to-be-proxied-to" }
    }
  ],
  "stubs": [
    {
      "requestMatcher": { "url": "/to-be-proxied-to" },
      "response": { "body": "it was proxied!", "type": "text" }
    }
  ]
}
